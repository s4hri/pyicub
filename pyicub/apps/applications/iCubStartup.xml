<application>
<name>iCubStartup</name>

        <dependencies>
        </dependencies>

        <module>
                <name>yarplogger</name>
                <parameters>--start --no_stop</parameters>
                <node>icubs4hri</node>
        </module>

        <module>
                <name>yarprobotinterface</name>
                <node>pc104</node>
        </module>

        <module>
                <name>iKinCartesianSolver</name>
                <parameters>--part right_arm</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                </dependencies>
        </module>

        <module>
                <name>iKinCartesianSolver</name>
                <parameters>--part left_arm</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                </dependencies>
        </module>

        <module>
                <name>iKinCartesianSolver</name>
                <parameters>--part right_leg</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                </dependencies>
        </module>

        <module>
                <name>iKinCartesianSolver</name>
                <parameters>--part left_leg</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                </dependencies>
        </module>

        <module>
                <name>iKinGazeCtrl</name>
                <parameters>--from config_imu_filter.ini</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                    <port timeout="60">/imuFilter/inertial:o</port>
                </dependencies>
        </module>

        <module>
                <name>wholeBodyDynamics</name>
                <parameters>--autoconnect --headV2</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                </dependencies>
        </module>

        <module>
                <name>gravityCompensator</name>
                <parameters>--headV2</parameters>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60">/wholeBodyDynamics/rpc:i</port>
                </dependencies>
        </module>

        <module>
                <name>fingersTuner</name>
                <node>icubs4hri</node>
                <dependencies>
                    <port timeout="60.0" request="is_ready" reply="[ok]">/icub/yarprobotinterface</port>
                </dependencies>
        </module>

        <module>
                <name>imuFilter</name>
                <node>pc104</node>
                <dependencies>
                    <port timeout="60">/icub/inertial</port>
                </dependencies>
        </module>

        <connection>
                <from>/icub/inertial</from>
                <to>/imuFilter/inertial:i</to>
                <protocol>tcp</protocol>
        </connection>

</application>
